{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User Microservice",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e ../.env.dev nodemon index.ts",
    "start": "npx prisma migrate deploy && node ./dist/user-service/index.js",
    "prisma:migrate": "dotenv -e ../.env.dev -- npx prisma migrate dev --name",
    "prisma:studio": "dotenv -e ../.env.dev -- npx prisma studio",
    "prisma:deploy": "dotenv -e ../.env.dev -- npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate"
  },
  "author": "roman-ojha",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/passport": "^1.0.15",
    "@types/passport-jwt": "^3.0.13",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
