server {
        server_name admin.journey.uk.to www.admin.journey.uk.to;
        location / {
                proxy_pass http://0.0.0.0:3000;
                proxy_set_header Host $host;
                proxy_set_header X-Real-IP $remote_addr;
                proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
                proxy_set_header X-Forwarded-Proto $scheme;
        }

    listen [::]:443 ssl; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/admin.journey.uk.to/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/admin.journey.uk.to/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = www.admin.journey.uk.to) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = admin.journey.uk.to) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


        listen 80;
        listen [::]:80;
        server_name admin.journey.uk.to www.admin.journey.uk.to;
